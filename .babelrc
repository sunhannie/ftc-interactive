{
  "env": {
    "development": {
      "presets": ["es2015", "react"],
      "plugins": [
        "react-hot-loader/babel",
        "transform-decorators-legacy"
      ]
    },
    "testing": {
      "presets": ["latest", "react"],
      "plugins": [
        "react-hot-loader/babel",
        "transform-decorators-legacy"
      ]
    },
    "production": {
      "presets": [
        ["latest"],
        "react"
      ],
      "plugins": [
        "transform-decorators-legacy"
      ]
    }
  }
  
}